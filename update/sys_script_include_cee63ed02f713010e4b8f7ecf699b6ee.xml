        <name>GetRecentRecords</name>
        <script><![CDATA[function getRecentRecords(){
	
	var list = [];
	var task = new GlideRecord("task");
	task.addActiveQuery();
	task.orderByDesc('sys_updated_on');
	task.setLimit(5);
	task.query();
	
	while(task.next()){
		list.push(task);
	}
	
	return list;
}



]]></script>
