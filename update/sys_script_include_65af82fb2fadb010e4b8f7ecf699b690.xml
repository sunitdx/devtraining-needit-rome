        <name>GetRequesterDetails</name>
        <script><![CDATA[var GetRequesterDetails = Class.create();
GetRequesterDetails.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	getData : function(){

		var usrRecord = new GlideRecord("sys_user");
		usrRecord.get(this.getParameter('sysparm_userId'));
		{
			var obj ={};
			obj.manager = usrRecord.manager+'';
			obj.phone =usrRecord.mobile_phone+'';

		}

		return JSON.stringify(obj);

	},

	type: 'GetRequesterDetails'
});]]></script>
